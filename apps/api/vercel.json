{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/getToken/(?<username>[^/]+)/(?<roomId>[^/]+)",
            "dest": "/api.py"
        },
        {
            "src": "/api/getAgentToken/(?<roomId>[^/]+)",
            "dest": "/api.py"
        }
    ]
}