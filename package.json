{
  "name": "livekits",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip install -r requirements.txt && python -m uvicorn apps.api.index:app --reload",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "turbo run build",
    "next-dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "concurrently": "^9.2.1"
  }
}